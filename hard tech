setcpm(128/4) deep

$: s("sawtooth!16")
.n("3 2 1 [11 15 21] 6 8".floor()).cutoff(rand.range(100,5000))
.lpf(6600)
.wt(.4)
.wtenv(.02)
.unison(.24)
.dec(.16)
.gain(4)
.postgain(.8)
.room(1.2).rsize(.01)
.shape(.12)
.detune("<.3 12.1 .5 24.1>")
.fm(.03)
.o(.03)
.n(absoluteOrientationAlpha.segment(4).range(7,6))
.n(accelerationZ.segment(2).range(6,12))
._scope()

$: s("sbd!4")
.duck(2)
.duckdepth(.2)
.o(.6)
.gain(1.7)
.sustain(4)
.hpf(10)
.color("red")._scope()

$: s("supersaw!4*2").cutoff(rand.range(100,4000))
 .n("2 4 [4 1] 1".floor())
 .dec(.2)
  .n(accelerationX.segment(7).range(2,14))
  .unison(.12)
  .wt(.7)
 .sustain(.8)
 .release(.14)
 .room(.6)
  .o(.06)
 .postgain(2)
  .gain(1)
 .color("red")._scope()

$: s("oh:2*8")
.wt(4)
.duck(3)
.sustain(3)
.hpf(30)
.orbit(6)
.gain(1)
.color("pink")._scope()



$: s("pulse!8*2")
.n("1 0 [2 1 2] 2").rev().cutoff(rand.range(100,2000))
.gain(2)
.postgain(1)
.wt(.02)
.unison(.07)
.dec(.2)
.room(.2)
.orbit(.07)
.o(.08)
.fm(.1)
.fmdecay(.04)
._scope()


$: note("d1!4").s("sine").penv(6).pdecay(.6).decay(.28).distort("4:.4")
  .gain(2).n(accelerationY.segment(1).range(4,7))
.color("blue")._scope().cutoff(rand.range(100,1000))



