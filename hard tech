setcpm(140/4) dark low

$: s("sbd").beat("0,7,10,3", 8)
.n(accelerationX.segment(2).range(0,9)).scale("d:minor").gain(1).postgain(1.2)
.note("d d f [a# c d] a# a").s("pulse").chorus(.5).sustain("<1*7>").duckorbit(5).duckattack(0.2).duckdepth(5)
 .n(orientationGamma.segment(1).range(0,2)).scale("c:minor")
  ._scope()


$: n(run(2)).scale("d:minor:pentatonic").s("sawtooth").delay(.6).orbit(6)
 .n(gravityY.segment(4).range(0,7)).scale("C:minor")
 .n(orientationAlpha.segment(7).range(0,7)).scale("C:minor").cutoff(rand.range(1000,9000)).note("<[c,eb,g]!2 [c,f,ab] [d,f,ab]>")
.arpWith(haps => haps[2]).iter(1)
 ._scope()

$: s("supersaw!4*2").cutoff(rand.range(100,4000))
 .n("2 4 [4 1] 1".floor()).attack("<0 .1 .5>")
 .dec(.2)
  .n(accelerationX.segment(9).range(1,14))
  .unison(.12)
  .wt(.7)
 .sustain(.8)
 .release(.14)
 .room(.6)
  .iter(2)
  .o(.06)
 .postgain(2)
  .gain(1)
 .color("red")._scope()


$: note("d1!4").s("sine").penv(2).pdecay(.6).decay(.28).distort("4:.4")
  .gain(2).n(accelerationY.segment(1).range(1,77))
.color("blue")._scope().cutoff(rand.range(100,1000))






