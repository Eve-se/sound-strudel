setcpm(120/4) 

$: s("sbd:3*4, oh:6*8, sd:4*2, cr:4*1")
  .gain(1.5)
 .shape(.2)
 .sustain(1.5)
 .hpf(120)
 .postgain(1)
  
 .color("blue")._scope()

$: s("supersaw").s("user")
  .fast(4)
 .chorus("<.5*4>")
  .seg(16)
  .scale("d:mayor")
  .delay(.5)
  .o(6)
 .lpq(slider(7.606 ,3 ,10))
 .sustain("<0.3*4>")
 .release("<0.6!4>")
 .shape(0.2)
 .lpenv(slider(9.03, 1, 12))
 .room(.4)
 .postgain(1.5)
 .color("red")._scope()


$: s("sawtooth").cutoff(rand.range(500,8000))
  .seg(3)
  .scale("e:minor")
  .gain(0.4)
  .fast(1.7)
  .delay(1.7)
  .o(6)
  .unison("<0.1*2>")
  .lpq(slider(4.173 ,3 ,20))
  .sustain(0.7)
  .release(0.3)
  .shape(0.6)
  .lpenv(slider(2.682, 1, 30))
  .room(.1)
  .postgain(1)
  .color("red")._scope()

$: s("wt_digital")
 .gain(1)
 .scale("a:mayor")
 .sustain(1.5)
 .seg(4)
 .room(.6)
 .delay(2.4)
 .fast(2)
 .release(0.5)
 .shape(0.3)
 .postgain(1.4)
 .color("white")._scope()

$: s("z_noise").s("zzfx")
 .seg(12)
  .scale("f:minor")
  .gain(0.2)
  .fast(0.6)
  .delay(0.4)
  .lpq(slider(3005 ,2500 ,5000))
  .sustain("<0.8*4>")
  .release(0.6)
  .shape(0.6)
  .lpenv(slider(22.982, 1, 30))
  .room(7)
  .chorus(5)
  .postgain(1.2)
  ._scope()

$: s("z_square").s("pulse")
 .seg(2)
  .scale("a:minor")
  .gain(1)
  .fast("<0.5*3>")
  .delay(0.3)
  .lpq(slider(3511.5 ,2500 ,11000))
  .sustain(0.4)
  .release(0.7)
  .shape(0.8)
  .lpenv(slider(4.596, 1, 30))
  .room(0.7)
  .postgain(0.5)
  ._scope()


